--
-- Name: matchseq; Type: SEQUENCE; Schema: public; Owner: howard
--

CREATE SEQUENCE matchseq
    START WITH 1000000
    INCREMENT BY 1
    MAXVALUE 9999999
    MINVALUE 1000000
    CACHE 1;


ALTER TABLE public.matchseq OWNER TO howard;

--
-- Name: matchseq; Type: SEQUENCE SET; Schema: public; Owner: howard
